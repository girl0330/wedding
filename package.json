    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "json-server": "^1.0.0-beta.3",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
